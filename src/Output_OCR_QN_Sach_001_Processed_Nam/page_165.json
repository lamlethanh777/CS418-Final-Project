        "Thư rằng:",
        "Lê Xù con mọn,",
        "Tay dọn bức thư,",
        "Kể nỗi duyên do,",
        "Trình cha mẹ biết.",
        "Vốn tôi đã quyết,",
        "Một tiết đồng trinh,",
        "Cha mẹ ép tình,",
        "Cho nên phải trốn,",
        "Lê thành đào động",
        "Gần bảy năm trời,",
        "Mới gặp năm người,",
        "Đi tìm đến đấy,",
        "Nó đã trông thấy,",
        "Song chẳng biết tôi,",
        "Đường sá xa xôi,",
        "Nó toan trở lại.",
        "Vàng bạc của cải,",
        "Chẩn thải cho tội",
        "Kể hết khúc nhối,",
        "Cùng tôi nói chuyện."
    ]
